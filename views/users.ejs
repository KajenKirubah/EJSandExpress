<%- include('includes/head.ejs') %>
<body>
  <%- include('includes/navigation.ejs') %>

  <main>
    <h1>Welcome to the Users Page</h1>
    <% if(users.length <= 0) { %>
    <p>Please Add Some Users !!</p>
    <% } else { %>
    <ul>
      <% for(let user of users) { %>
      <li><%= user.name %></li>
      <% } %>
    </ul>
    <% } %>
  </main>

  <%- include('includes/end.ejs') %>

